{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "deb822",
  "word": "field_name",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "paragraph"
      }
    },
    "escape_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_hspace"
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "field_name": {
      "type": "PATTERN",
      "value": "[!\"$-,\\.-9;-~][!-9;-~]*"
    },
    "folded_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_hspace"
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "_hspace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "statement"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_separator_line"
          }
        }
      ]
    },
    "simple_value": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "field_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_line"
              },
              {
                "type": "SYMBOL",
                "name": "folded_line"
              }
            ]
          }
        }
      ]
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "SYMBOL",
          "name": "field_value"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "STRING",
      "value": "\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_separator_line"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}